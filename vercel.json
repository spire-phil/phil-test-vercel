{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder", 
      "config": {
        "serverFiles": ["server-middleware/**", "api/**"]
      }
    }                                  
  ],                                    
  "rewrites": [
    {
      "source": "/gettime",
      "destination": "/api/gettime"
    },
  ]
}
